package com.example.assign;

import com.example.thegreat.R;

import android.support.v7.app.ActionBarActivity;
import android.support.v7.app.ActionBar;
import android.support.v4.app.Fragment;
import android.os.Bundle;
import android.view.LayoutInflater;
import android.view.Menu;
import android.view.MenuItem;
import android.view.View;
import android.view.ViewGroup;
import android.widget.Button;
import android.widget.EditText;
import android.widget.TextView;
import android.os.Build;

public class Addition extends ActionBarActivity {

	TextView tvnumberone, tvnumbertwo,tvresult;
	EditText etnumberone,etnumbertwo;
	Button btnback,btnadd;

	@Override
	protected void onCreate(Bundle savedInstanceState) {
		super.onCreate(savedInstanceState);
		setContentView(R.layout.activity_addition);
		 tvnumberone=(TextView) findViewById(R.id.numberone);
	      tvnumbertwo=(TextView) findViewById(R.id.numbertwo);
	      tvresult=(TextView) findViewById(R.id.result);
	      
	      etnumberone=(EditText) findViewById(R.id.numberone);
	      etnumbertwo=(EditText) findViewById(R.id.numbertwo);
	      
	      btnback=(Button) findViewById(R.id.back);
	      btnadd=(Button) findViewById(R.id.add);
	      
	      btnback.setOnClickListener(this);
	      btnadd.setOnClickListener(this);
	      

		

}
}
